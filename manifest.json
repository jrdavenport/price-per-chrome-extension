{
  "name": "Price Per",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Highlight items based on their unit costs relative to each other",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/js/content_script.js"]
    }
  ],
  "permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "src/js/*"
  ]
}
