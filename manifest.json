{
  "name": "Price Per",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Highlight items based on their unit costs relative to each other",
  "content_scripts": [
    {
      "matches": ["*://*.tesco.com/*"],
      "js": ["src/js/content_script.js"]
    }
  ],
  "permissions": ["*://*.tesco.com/*"],
  "web_accessible_resources": [
    "src/js/*"
  ]
}
